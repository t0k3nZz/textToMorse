#include<iostream>
#include<string>
#include <locale>
#include<Windows.h>
#include<MMSystem.h>

using namespace std;

void convert(char);

int main()
{
	string sentance = "";
	cout << "Enter a sentance to be translated into morese code: ";
	cin >> sentance;

	for (unsigned int i = 0; i < sentance.length(); i++)
	{
		if (sentance[i] != ' ')
		{
			convert(sentance[i]);
			PlaySound(TEXT("space.wav"), NULL, SND_SYNC);
		}
		else
			PlaySound(TEXT("seperate.wav"), NULL, SND_SYNC);
	}
	system("pause");
	return 0;
}
void convert(char c)
{
